{
  "compilerOptions": {
    "outDir": "build",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": true,
    "target": "es5",
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "paths": {
      "cheerio": [
          "./node_modules/cheerio/dist/browser/slim.d.ts"
      ]
    }
  },
  "exclude": ["node_modules", "build"]
}
